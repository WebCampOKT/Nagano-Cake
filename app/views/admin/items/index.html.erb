<div class="container">
  <div class="row justify-content-between">
    <div class="col-4 mt-5 mb-3">
      <h4><span style="background: #FFEBD2;">商品一覧</span></h4>
    </div>
    <div class="col-4 mt-4 text-right">
      <%= link_to new_admin_item_path do %>
        <%= image_tag 'cake.png', class: "plus-button" %>
      <% end %>
    </div>
  </div>
  <%= render 'item_index', items: @items %>
  <div class="pagination justify-content-center">
    <%= paginate @items %>
  </div>
</div>